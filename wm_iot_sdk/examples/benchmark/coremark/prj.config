
#
# Compiler configuration
#
CONFIG_COMPILER_OPTIMIZE_LEVEL_O3=y
# end of Compiler configuration

#
# FreeRTOS
#
CONFIG_FREERTOS_HZ=1000
# CONFIG_FREERTOS_USE_TRACE_FACILITY is not set
# CONFIG_FREERTOS_ENABLE_RUN_TIME_STATS is not set
# CONFIG_FREERTOS_ENABLE_IDLE_HOOK is not set
# CONFIG_FREERTOS_ENABLE_TICK_HOOK is not set
CONFIG_FREERTOS_TIMER_TASK_PRIORITY=1
CONFIG_FREERTOS_ASSERT_DISABLE=y
# CONFIG_FREERTOS_CHECK_STACKOVERFLOW is not set
# end of FreeRTOS
