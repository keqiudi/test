config SYS_CLOCK_MAX_TIMEOUT_DAYS
	int
	range 0 366
	default 365
	help
	  Internal use, any modification is not allowed.
	  
config SYS_CLOCK_TICKS_PER_SEC
    int
	range 100 1000
	default FREERTOS_HZ
	help
	  Internal use, any modification is not allowed.  
